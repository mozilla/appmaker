<div class="page-wrapper">
  <nav class="top-nav clearfix">
    <div class="logo"></div>
    <div class="cards">
      <div class="card-list"></div>
      <div class="add-card">+</div>
    </div>
    <div class="nav-ui">
      <button class="btn-std btn-md publish btn-critical">Publish</button>
      <button class="btn-std btn-md new">New App</button>
    </div>
  </nav>
  <div class="container animate-me">
    <div class="inner-container clearfix">
      <div class="listen-column"></div>
      <div class="phone-container">
        <div class="phone-inner">
          <div class="phone-top"></div>
          <div class="phone-border">
            <% if (htmlInjection.length > 0) { -%>
              <%- htmlInjection %>
            <% } else { -%>
              <div id="flathead-app">
                <div class="ceci-card">
                  <div class="fixed-top"></div>
                  <div class="phone-canvas"></div>
                  <div class="fixed-bottom"></div>
                </div>
              </div>
            <% } -%>
          </div>
          <div class="phone-shadow"></div>
          <div class="phone-bottom"></div>
          <div class="garbage-bin"></div>
        </div>
      </div>
      <div class="broadcast-column">
      </div>
    </div>
  </div>

  <div class="tray app-column animate-me">
    <div class="expand-handle"></div>
    <div class="modal-header">
      <input type="text" class="component-search" placeholder="Search components..." />
    </div>
    <div id="components-wrapper">
      <div id="components"></div>
    </div>
  </div>

  <div class="right-column app-column animate-me">
    <div class="expand-handle"></div>
    <div class="inner-wrapper">
      <div class="editable-section">
        <div class="remix-ui">
          <h3>
            Remixing
            <a class="cancel-remix" title="Cancel remixing this widget." href="#">Cancel</a>
            <a class="finish-remix" title="Refresh the widget to see your changes." href="#">Done</a>
            <a class="save-remix" title="Refresh the widget to see your changes." href="#">Save</a>
          </h3>
          <div class="remix-wrapper">
            <input class="component-name" type="text" placeholder="Component Name" value="my-remixed-widget"/>
          </div>
        </div>
      <div class="editable-header nonremix-ui">
        <span class="name"></span>
      </div>
      <div class="trey-tabs">
        <a class="selected-tab" tab="customize" href="#">Customize</a>
        <a tab="view-source" href="#">View Source</a>
      </div>
      <div class="tab-sections">
        <div class="section section-customize">
          <div class="editable-attributes"></div>
        </div>
        <div class="section section-view-source">
          <div class="view-source-menu">
            <a source="HTML" class="selected" href="#">HTML</a>
            <a source="JS" href="#">JS</a>
            <a source="CSS" href="#">CSS</a>
            <a source="info" href="#">Info</a>
          </div>
          <div class="view-source-items">
            <p class="remix-helper">
              Edit the source to Remix this component!
              - <a class="dismiss-note" href="#">OK!</a>
            </p>
            <div class="source-type" source="HTML">
              <textarea><p>Here is some HTML to edit</p></textarea>
            </div>
            <div class="source-type" source="JS">
              <textarea source="JS">widget.load();</textarea>
            </div>
            <div class="source-type" source="CSS">
              <textarea>.widget { display: awesome:}</textarea>
            </div>
            <div class="source-type" source="info">
              <label>Component Description</label>
              <input type="text" value="This is the component description" />
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

  <div class="log-wrapper">
    <div class="log">
      <ul class="scroll"></ul>
      <button class="log-toggle"></button>
    </div>
  </div>

  <div class="channel-menu channel-menu-template">
    <div class="hover-helper"></div>
    <div class="channel-option subscription-option channel-template">
      <div class="color-ui">
        <div name="blue"      color="blue"    class="color" data-channel="blue"></div>
        <div name="green"     color="green"   class="color" data-channel="green"></div>
        <div name="yellow"    color="yellow"  class="color" data-channel="yellow"></div>
        <div name="orange"    color="orange"  class="color" data-channel="orange"></div>
        <div name="red"       color="red"     class="color" data-channel="red"></div>
        <div name="pink"      color="pink"    class="color" data-channel="pink"></div>
        <div name="purple"    color="purple"  class="color" data-channel="purple"></div>
        <div name="false"     color="false"   class="color" data-channel=""></div>
      </div>
      <label><span class="channel-name"></span><div color="blue" class="chosen-color"></div></label>
    </div>
    <div class="arrow"></div>
  </div>

  <div class="modal-wrapper hidden flex">
    <div class="modal" id="component-discovery-modal">
      <div class="modal-header">
        Search <input type="text" class="component-search" placehodler="Search for a component" />
        <button class="done">Done</button>
      </div>
      <div id="components-wrapper">
        <div id="components"></div>
      </div>
    </div>
    <div class="modal publishdialog">
      <div class="modal-inner">
        <div class="spinner hidden">
          <h2>Loading…</h2>
          <div class="clearfix">
            Publishing your app on the internets.
          </div>
        </div>
        <div class="failure hidden">
          <h2>Uh-oh…</h2>
          <div class="clearfix">
            Your app couldn't be published.  The server said:
            <pre class="message"></pre>
          </div>
        </div>
        <div class="success hidden">
          <h2>Success!</h2>
          <div class="clearfix">
            Your app has been published to <a class="modal-publish-link" target="_blank"></a>
          </div>
        </div>
        <button class="btn-std return-btn">
          Return to app
        </button>
      </div>
    </div>
  </div>

  <div class="game-status">
    <div class="wrapper level-finished">
      <h2>Congratulations!</h2>
      <h1>You completed Level <span class="level-number"></span></h1>
      <div>
      <p>You've been promoted to...</p>
      <div class="player-rank"></div>
      </div>
      <a class="next-level green-button" href="#">Continue</a>
      <div class="mailman-head"></div>
    </div>

    <div class="wrapper level-intro">
      <h2 class="level-number"></h2>
      <h1 class="level-name"></h1>

      <div class="level-indicator">
        <div class="level-progress"></div>
        <div class="current-level"></div>
      </div>

      <h3 class="objective">Your Mission</h3>
      <div class="step-list"></div>
      <a class="start green-button" href="#">START</a>
      <div class="sub-links">
        <a class="optout" href="#">I don't want to play :(</a> - <a class="restart" href="#">Start Over</a>
      </div>
      <div class="mailman-head"></div>
    </div>

    <div class="wrapper game-finished">
      <h2>You win!</h2>
      <h1>You've been promoted to Postmaster!</h1>
      <p>Go forth and build apps!</p>
      <a class="green-button endgame" href="#">I'm Ready</a>
    </div>


    <div class="wrapper level-status">
      <a class="optout" href="#">I quit!</a>
      <a class="show-hints" href="#"></a>
      <div><span class="level-number"></span> - <span class="level-name"></span></div>
      <div class="step-list"></div>
      <div class="hint">
        <strong>Hint:</strong> <span class="hint-text">What you gotta do, is what you gotta do!</span>
        <div class="arrow"></div>
      </div>
    </div>
  </div>
</div>
<input id="appmaker-remix-url" type="hidden" value="<%- publishUrl %>" />
