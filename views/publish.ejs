<!DOCTYPE html>
<html>
  <head>
    <title>Published Flathead Project</title>
    <link rel="stylesheet" href="<%= process.env.ASSET_HOST %>/stylesheets/style.css" />
    <link href="http://fonts.googleapis.com/css?family=Source+Sans+Pro:300, 400,600" rel="stylesheet" type="text/css">
    <link rel="component" type="text/ceci" href="<%= process.env.COMPONENTS_BASE_URL %>/package/all">
    <script src="<%= process.env.ASSET_HOST %>/vendor/requirejs/require.js"></script>
    <script>
      (function(){
        var requireCtx = require.config({
          baseUrl: '<%= process.env.ASSET_HOST %>'
        });

        requireCtx(['vendor/ceci/ceci'], function (Ceci) {
          Ceci.load(function (components) {
          });
        });
      })();
    </script>
  </head>
  <body>
    <div id="flathead-app">
      <% for (var i = 0, card; card = cards[i], i < cards.length; ++i) { %>
        <div class="ceci-card" id="ceci-card-<%= i+1 %>">
          <div class="phone-canvas">
            <% for (var j = 0, element; element = card.elements[j], j < card.elements.length; ++j) { %>
              <<%= element.tagname %> id="<%= element.id %>">
                <% if (element.broadcast !== 'false') { %>
                  <broadcast on="<%= element.broadcast %>"></broadcast>
                <% } %>
                <% for (var k = 0, listen; listen = element.listen[k], k < element.listen.length; ++k) { %>
                  <listen on="<%= listen.channel %>" for="<%= listen.listener %>"></listen>
                <% } %>
              </<%= element.tagname %>>
            <% } %>
          </div>
        </div>
      <% } %>
    </div>
  </body>
</html>
