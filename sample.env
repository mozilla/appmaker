FORCE_SSL=

# Security headers with helmet
# Use HSTS? (set to true to enable HSTS)
HSTS_ENABLED=

# Use Deny settings for X-Frame headers?
# set to true to Set X-Frame headers to deny
XFO_HEADERS_DENY=

# Use default XSS protection?
# set to true to enable IEXSS protection
IEXSS_PROTECTION_ENABLED=
